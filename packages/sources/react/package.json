{
  "name": "@vtmn/react",
  "version": "0.12.0",
  "description": "Decathlon Design System - Vitamin React components library",
  "keywords": [
    "design-system",
    "ui-kit",
    "libraries",
    "react",
    "vitamin"
  ],
  "homepage": "https://github.com/Decathlon/vitamin-web/tree/main/packages/sources/react",
  "bugs": {
    "url": "https://github.com/Decathlon/vitamin-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Decathlon/vitamin-web.git"
  },
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "start": "npm run build -- -w",
    "test:ci": "CI=true npm run test:coverage",
    "test:coverage": "react-scripts test --env=jsdom --coverage --watchAll=false"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/index.ts",
      "!src/typings.ts",
      "!src/**/*.test.ts,",
      "!src/**/*.d.ts",
      "!src/**/*.stories.tsx"
    ]
  },
  "devDependencies": {
    "@babel/runtime": "^7.14.8",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "^8.2.5",
    "@rollup/plugin-url": "^6.0.0",
    "@svgr/rollup": "^5.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vtmn/icons": "^0.5.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-plugin-typescript-to-proptypes": "^1.4.2",
    "enzyme": "^3.11.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "fast-check": "^2.11.0",
    "gh-pages": "^3.2.1",
    "glob": "^7.2.0",
    "handlebars": "^4.7.6",
    "jest-fetch-mock": "^3.0.3",
    "postcss": "^8.3.8",
    "postcss-custom-media": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.57.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ]
}
