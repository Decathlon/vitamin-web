{
  "name": "@vtmn/icons",
  "version": "0.1.1",
  "description": "Decathlon Design System - Vitamin icons library",
  "keywords": [
    "design-system",
    "ui-kit",
    "libraries",
    "icons",
    "vitamin"
  ],
  "homepage": "https://github.com/Decathlon/vitamin-web/tree/main/packages/sources/icons",
  "bugs": {
    "url": "https://github.com/Decathlon/vitamin-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Decathlon/vitamin-web.git"
  },
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "chokidar 'src/**' --ignore '**/dist/**' -c 'yarn build'",
    "prebuild": "svgo -f src/vitamix",
    "build": "yarn build:svg && yarn build:font && yarn build:sprite",
    "build:svg": "node src/build-svg.js",
    "build:font": "mkdir -p ./dist/vitamix/font && fantasticon --normalize",
    "build:sprite": "mkdir -p ./dist/vitamix/sprite && spritesh -i src/vitamix -o dist/vitamix/sprite/vitamix.svg"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "fantasticon": "^1.1.1",
    "fs-extra": "^9.1.0",
    "spritesh": "^1.2.1",
    "svgo": "^2.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ]
}
