    "@vtmn/css-popover": "^0.5.6",
    "@vtmn/css-text-input": "^0.13.8"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.2",
    "chokidar-cli": "^3.0.0",
    "eslint": "8.9.0",
    "eslint-plugin-svelte3": "3.4.1",
    "rollup": "^2.67.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "3.46.4",
    "svelte-preprocess": "4.10.4"
  },
  "peerDependencies": {
    "svelte": "^3.34.0"
  },
  "publishConfig": {
    "access": "public"
